{
  "name": "consumer-parcel",
  "type": "module",
  "source": "src/index.js",
  "main": "dist/main.js",
  "dependencies": {
    "intermediary": "0.0.1",
    "@connectrpc/connect": "^1.3.0"
  },
  "devDependencies": {
    "parcel": "^2.9.3"
  },
  "targets": {
    "main": {
